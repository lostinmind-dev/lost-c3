{
    "name": "@lost-c3/lib",
    "version": "1.0.6",
    "exports": {
        ".": "./mod.ts",
        "./cli": "./cli.ts"
    },
    "tasks": {
        "uninstall:cli": "deno uninstall lost -g",
        "install:cli:local": "deno install --name lost ./cli.ts --global -f -A",
        "test:cli": "deno run -A ./cli.ts"
    },
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true
    }
}
